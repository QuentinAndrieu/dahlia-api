  },
  "homepage": "https://github.com/QuentinAndrieu/dahlia-api#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-eslint": "8.0.3",  
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.62.0",
    "flow-remove-types": "^1.2.3",
    "flow-typed": "^2.2.3"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
